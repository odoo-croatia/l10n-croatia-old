<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="base.hr" model="res.country">
            <field name="address_format"
                   eval="'%(street)s\n%(street2)s\n%(zip)s %(city)s\n%(country_name)s'"/>
        </record>

        <record id="base.europe" model="res.country.group">
            <field name="name">EU</field>
        </record>

    </data>
</odoo>